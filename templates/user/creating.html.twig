{% extends 'base.html.twig' %}
    {% block content %}
        <div class="container">
            <h2 class="mt-7 pt5" style="padding-top: 4em;">New Customer</h2>
            <p>Is being created...</p>

            {% for label, messages in app.flashes %}
                {% for message in messages %}
                    <div class="alert alert-{{ label }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
            <div class="container p-4 pb-0">
                <section class="mb-4">
                    <!-- Facebook -->
                    <a class="btn btn-outline-dark btn-floating m-1" href="/compare" role="button"><i class="">Next Step</i></a>

                </section>
            </div>
        </div>

    {% endblock %}
